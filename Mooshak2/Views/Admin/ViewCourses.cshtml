@model List<Mooshak2.Models.Entity.Course>
@{
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<div class="container-fluid" id="fluid">
    <div class="row">

        <div class="col-md-1"></div>
        
            <div class="col-md-8">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h2 class="navtitle">A list of all courses </h2>
                    </div>

                    <form action="ViewCourses" method="POST" class="form-horizontal">
                        <div class="created-view-div">
                            <table class="table">
                                <tr>
                                    <th>Course</th>
                                    <th>Teacher</th>
                                </tr>
                                @foreach (var item in @Model)
                                {
                                    <tr>
                                        <td>@item.courseName  </td>
                                        <td>@item.courseTeacher</td>
                                    </tr>
                                }
                            </table>

                            @*************************************

                                <table class="table">
                                    <tr>
                                        <th>Name</th>
                                        <th>SSN</th>
                                        <th>Email</th>
                                    </tr>
                                    @foreach (var item in Model.teachers)
                                    {
                                    <tr>
                                        <td>@item.name</td>
                                        <td>@item.ssn</td>
                                        <td>@item.email</td>
                                    </tr>
                                    }
                                </table>




        **********************************************@





                        </div>
                    </form>

                </div>
            </div>
    </div>
</div>
